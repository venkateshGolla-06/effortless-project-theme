{% comment %}
  Renders cart drawer

  Usage:
  {% render 'cart-drawer' %}
{% endcomment %}

{{ 'quantity-popover.css' | asset_url | stylesheet_tag }}
{{ 'component-card.css' | asset_url | stylesheet_tag }}
{{ 'custombestsellers.css' | asset_url | stylesheet_tag }}



<script src="{{ 'cart.js' | asset_url }}" defer="defer"></script>
<script src="{{ 'quantity-popover.js' | asset_url }}" defer="defer"></script>

<style>
  .drawer {
    visibility: hidden;
  }

  .cart-progress-bar{
    height:0.8rem;
    background:white;
    border-radius: 20px
  }
  .in-stock-bar{
    background:#00d964; 
    height:100%;
    border-radius:20px;
    display:block"
  }
  
  .drawer .container {
    color: #333;
    margin: 0 auto;
    text-align: center;
  }
  .sub-cart-drawer-heading{
   font-size:14px;
   font-weight:bold;
  }
  
 .sub-cart-drawer{
  position: absolute;
  z-index: 1;
  top: 70px;
  bottom: 0;
  right: 0;
  width: 200px;
  padding: 34px;
  border-right: 0;
  background-color: #fff;
  overflow: scroll;
  display: flex;
  flex-direction: column;
  align-items:center;
 transition: all .6s ease; 
 border-top-left-radius:8px;
  }
  .drawer.active sub-cart-drawer.active{
    transform: translate(-600px);
  }
  @media screen and (max-width:900px){
    sub-cart-drawer{
    display:none;
    
    }
  
  }
  .productlist-grid-view{
   display:grid;
   grid-template-rows:1fr auto;
   row-gap:20px;
  }
  sub-cart-drawer::-webkit-scrollbar {
    width: 5px;
    height: 5px;
}

sub-cart-drawer::-webkit-scrollbar-thumb {
    background: #888;
    border-radius: 10px;
}

sub-cart-drawer::-webkit-scrollbar-thumb:hover {
    background: #555;
}

sub-cart-drawer::-webkit-scrollbar-track {
    background: #f1f1f1;
}

</style>








<cart-drawer class="drawer{% if cart == empty %} is-empty{% endif %}">
  <div id="CartDrawer" class="cart-drawer">
    <div id="CartDrawer-Overlay" class="cart-drawer__overlay"></div>
    <sub-cart-drawer class="sub-cart-drawer">
      <p class="sub-cart-drawer-heading"> You also might like</p>
      <div class="productlist-grid-view">
        {% for product in settings.product_view_list %}
      <div class="">
        {% render 'product-card', product:product %}
      </div>
          {%endfor %}
      </div>
    
    
    
    </sub-cart-drawer >
    <div
      class="drawer__inner gradient color-{{ settings.cart_color_scheme }}"
      role="dialog"
      aria-modal="true"
      aria-label="{{ 'sections.cart.title' | t }}"
      tabindex="-1"
    >
      {%- if cart == empty -%}
        <div class="drawer__inner-empty">
          <div class="cart-drawer__warnings center{% if settings.cart_drawer_collection != blank %} cart-drawer__warnings--has-collection{% endif %}">
            <div class="cart-drawer__empty-content">
              <h2 class="cart__empty-text">{{ 'sections.cart.empty' | t }}</h2>
              <button
                class="drawer__close"
                type="button"
                onclick="this.closest('cart-drawer').close()"
                aria-label="{{ 'accessibility.close' | t }}"
              >
                {% render 'icon-close' %}
              </button>
              <a href="{{ routes.all_products_collection_url }}" class="button">
                {{ 'general.continue_shopping' | t }}
              </a>

              {%- if shop.customer_accounts_enabled and customer == null -%}
                <p class="cart__login-title h3">{{ 'sections.cart.login.title' | t }}</p>
                <p class="cart__login-paragraph">
                  {{ 'sections.cart.login.paragraph_html' | t: link: routes.account_login_url }}
                </p>
              {%- endif -%}
            </div>
          </div>
          {%- if settings.cart_drawer_collection != blank -%}
            <div class="cart-drawer__collection">
              {% render 'product-card', card_collection: settings.cart_drawer_collection, columns: 1 %}
            </div>
          {%- endif -%}
        </div>
      {%- endif -%}
      <div class="drawer__header">
        <h2 class="drawer__heading">{{ 'sections.cart.title' | t }} ({{ cart.item_count }}) Items </h2>
        <button
          class="drawer__close"
          type="button"
          onclick="this.closest('cart-drawer').close()"
          aria-label="{{ 'accessibility.close' | t }}"
        >
          {% render 'icon-close' %}
        </button>
      </div>
      {% if cart.item_count !=0 %}
        <div class="drawer__header-with-background">
          {% assign widthpercent=cart.total_price | money_without_currency | times : 100 | divided_by:20000  %}
